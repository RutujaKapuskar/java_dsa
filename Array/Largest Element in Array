//largest element in array
import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        
        System.out.println("Try programiz.pro");
       
        
        int arr[]= {3,2,1,5,6};
        int largest=arr[0];
        for(int i=1;i<arr.length;i++){
            if(largest<arr[i]){
                largest=arr[i];
                
            }
        }
        System.out.println(largest);
    }
}
